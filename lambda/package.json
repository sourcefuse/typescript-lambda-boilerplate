{
  "name": "lambda-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --ext .js,.ts,.tsx ./src/ ./tests/",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx ./src/ ./tests/",
    "script:build-dependency-layer": "yarn install --prod && ../scripts/build-dependency-layer.sh && yarn install;",
    "build": "tsc && yarn script:build-dependency-layer && cp -a dist/. ../terraform/dist/",
    "cleanup": "rm -rf dist/* && cd ../terraform && rm -rf dist/*",
    "test": "ts-mocha --timeout 120000 -p tsconfig.json \"tests/**/**/*.spec.ts\""
  },
  "repository": "https://github.com/sourcefuse/typescript-lambda-boilerplate",
  "author": "SourceFuse",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/archiver": "5.3.1",
    "@types/aws-lambda": "^8.10.93",
    "@types/chai": "4.3.0",
    "@types/minimist": "^1.2.2",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.21",
    "@types/unzipper": "^0.10.5",
    "@typescript-eslint/eslint-plugin": "5.13.0",
    "@typescript-eslint/parser": "5.13.0",
    "chai": "4.3.6",
    "eslint": "8.10.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "mocha": "9.2.1",
    "mocha-junit-reporter": "2.0.2",
    "prettier": "2.5.1",
    "ts-mocha": "9.0.2",
    "ts-node": "10.5.0",
    "tsc": "2.0.4",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.55.0",
    "@aws-sdk/client-s3": "3.53.1",
    "@aws-sdk/client-secrets-manager": "3.53.0",
    "aws-sdk": "2.1083.0"
  }
}
